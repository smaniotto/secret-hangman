{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "StatusResponse",
  "type": "object",
  "required": [
    "remaining_guesses",
    "word_length",
    "word_reveal"
  ],
  "properties": {
    "remaining_guesses": {
      "type": "integer",
      "format": "uint8",
      "minimum": 0.0
    },
    "word_length": {
      "type": "integer",
      "format": "uint8",
      "minimum": 0.0
    },
    "word_reveal": {
      "type": "string"
    }
  }
}
